<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleIndicator">
        <attr name="ci_radius" format="dimension" />
        <attr name="ci_margin" format="dimension" />
        <attr name="ci_background" format="color|integer" />
        <attr name="ci_selected_background" format="color|integer" />
        <attr name="ci_gravity">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
        <attr name="ci_mode">
            <enum name="inside" value="0" />
            <enum name="outside" value="1" />
            <enum name="solo" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SimplePagerTitleStrip">
        <attr name="pts_radius" format="dimension" />
        <attr name="pts_margin" format="dimension" />
        <attr name="pts_background" format="color|integer" />
        <attr name="pts_selected_background" format="color|integer" />
        <attr name="pts_gravity">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>

    <attr name="webview_progress_bg_color" format="reference|color"/>
    <attr name="webview_progress_color" format="reference|color"/>

</resources>